
public class School {
	int English;
	int  Math;
	public int average;
	public int getEnglish() {
		return English;
	}
	public int setEnglish(int i) {
		return English = i;
		
	}
	public int getMath() {
		return Math;
	}
	public int setMath(int math) {
		return Math = math;
	}
	public void build()
	{
		average=(math+i)/2;
	}


}
